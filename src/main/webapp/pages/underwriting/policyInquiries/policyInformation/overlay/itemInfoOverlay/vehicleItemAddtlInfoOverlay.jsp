<div style="margin-top:15px;text-align:center">
	<input type="button" class="button" id="btnReturnFromVehicleItemAddtlinfo" value="Return" style="width:18%"/>
	<input type="button" class="button" id="btnVehicleAccessories" value="Accessories" style="width:18%"/>
	<input type="button" class="button" id="btnVehicleDeductibles" value="Deductibles" style="width:18%"/>
	<input type="button" class="button" id="btnVehicleMortgagee" value="Mortgagee" style="width:18%"/>
	<input type="button" class="button" id="btnVehiclePicOrVid" value="View Picture or Video" style="width:18%"/>
</div>

<script>
	var moduleId = $F("hidModuleId"); //added by Kris 03.07.2013
	
	$("btnReturnFromVehicleItemAddtlinfo").observe("click", function(){
		overlayVehicleItemAdditionalInfo.close();
	});
	
	try{
		var objVehicleInfo = JSON.parse('${vehicleInfo}'.replace(/\\/g, '\\\\'));
		var mcItem = JSON.parse('${mcItem}'.replace(/\\/g, '\\\\'));
		$("btnReturnFromVehicleItemAddtlinfo").value = moduleId == "GIPIS101" ? "Return" : "Item Information";
	
		if(objVehicleInfo != null){
			if(moduleId == "GIPIS101") {
				$("tr2").hide();
				$("tr3").hide();
				$("tr4").hide();
				$("tr5").hide();
				
				$("txtVehicleItemNumber").value		= mcItem.itemNo;
				$("txtVehicleItemTitle").value		= unescapeHTML2(mcItem.itemTitle);
				$("txtVehicleAssignee2").value		= unescapeHTML2(objVehicleInfo.assignee);
				$("txtVehicleCOCType2").value		= objVehicleInfo.cocType;
				$("txtVehicleCOCSerialNo2").value	= formatNumberDigits(nvl(objVehicleInfo.cocSerialNo,0), 7);
				$("txtVehicleCOCYy2").value			= objVehicleInfo.cocYy;
				$("txtVehicleAcquiredFrom2").value	= unescapeHTML2(objVehicleInfo.acquiredFrom);
				$("txtVehicleTypeOfBody2").value	= unescapeHTML2(objVehicleInfo.typeOfBody);
				$("txtVehicleMotorType").value		= unescapeHTML2(objVehicleInfo.motorTypeDesc);
			} else {
				$("tr2GIPIS101").hide();
				$("tr3GIPIS101").hide();
				
				$("txtVehicleItemNumber").value		= objVehicleInfo.itemNo;
				$("txtVehicleItemTitle").value		= unescapeHTML2(objVehicleInfo.itemTitle);
				$("txtVehicleAssignee").value		= unescapeHTML2(objVehicleInfo.assignee);
				$("txtVehicleCOCATCNNo").value		= objVehicleInfo.cocAtcn;
				$("txtVehicleAcquiredFrom").value	= unescapeHTML2(objVehicleInfo.acquiredFrom);
				$("txtVehicleOrigin").value			= unescapeHTML2(objVehicleInfo.origin);
				$("txtVehicleDestination").value	= unescapeHTML2(objVehicleInfo.destination);
				$("txtVehicleCOCType").value		= objVehicleInfo.cocType;
				$("txtVehicleCOCSerialNo").value	= formatNumberDigits(objVehicleInfo.cocSerialNo, 7);
				$("txtVehicleCOCYy").value			= objVehicleInfo.cocYy;
				$("txtVehicleTypeOfBody").value		= unescapeHTML2(objVehicleInfo.typeOfBody);
				$("txtVehicleMotorType").value		= unescapeHTML2(objVehicleInfo.typeDesc);
			}
			
			$("txtVehicleMotorCoverage").value	= unescapeHTML2(objVehicleInfo.motorCoverage);
			$("txtVehiclePlateNo").value		= unescapeHTML2(objVehicleInfo.plateNo);
			$("txtVehicleModelYear").value		= objVehicleInfo.modelYear;
			$("txtVehicleCarCompany").value		= unescapeHTML2(objVehicleInfo.carCompany);
			$("txtVehicleMVFileNo").value		= unescapeHTML2(objVehicleInfo.mvFileNo);
			$("txtVehicleNumberOfPass").value	= objVehicleInfo.noOfPass;
			$("txtVehicleMake").value			= unescapeHTML2(objVehicleInfo.make);
			$("txtVehicleBasicColor").value		= unescapeHTML2(objVehicleInfo.basicColor);
			$("txtVehicleColor").value			= unescapeHTML2(objVehicleInfo.color);
			$("txtVehicleEngineSeries").value	= unescapeHTML2(objVehicleInfo.engineSeries);
			$("txtVehicleTowingLimit").value	= formatCurrency(objVehicleInfo.towing);
			
			$("txtVehicleUnladenWt").value		= objVehicleInfo.unladenWt;
			$("txtVehicleDeductible").value		= formatCurrency(objVehicleInfo.deductible);
			$("txtVehicleSerialNo").value		= unescapeHTML2(objVehicleInfo.serialNo);
			$("txtVehicleSublineType").value	= unescapeHTML2(objVehicleInfo.sublineTypeDesc);
			$("txtVehicleRepairLimit").value	= formatCurrency(objVehicleInfo.repairLim);
			$("txtVehicleMotorEngineNo").value	= unescapeHTML2(objVehicleInfo.motorNo);
			
		}
		
		$("btnVehicleAccessories").observe("click", function(){
			if(moduleId == "GIPIS101") {
				overlayVehicleAccessories = Overlay.show(contextPath+"/GIXXMcaccController", {
					urlContent: true,
					urlParameters: {
						action 	 	: "getGIXXMcaccTG",
						extractId 	: nvl($("hidItemExtractId").value,0),
						itemNo		: nvl($("hidItemNo").value,0) 
					},
					title: "Accessories",
					width: 500,
					height: 250,
					draggable: true,
					showNotice: true
				  });
			} else {
				overlayVehicleAccessories = Overlay.show(contextPath+"/GIPIMcaccController", {
					urlContent: true,
					urlParameters: {
						action 	 	: "getVehicleAccessories",
						policyId 	: nvl($("hidItemPolicyId").value,0),
						itemNo		: nvl($("hidItemNo").value,0) 
					},
					title: "Accessories",
					width: 500,
					height: 250,
					draggable: true,
					showNotice: true
				  });
			}
			
		});
		
	
		$("btnVehicleDeductibles").observe("click", function(){
			moduleId == "GIPIS101" ? getItemDeductibleList2(nvl($("hidItemExtractId").value,0),nvl($("hidItemNo").value,0)) :
									 getItemDeductibleList(nvl($("hidItemPolicyId").value,0),nvl($("hidItemNo").value,0));
		});
	
		$("btnVehicleMortgagee").observe("click", function(){
			moduleId == "GIPIS101" ? getItemMortgagees2(nvl($("hidItemExtractId").value,0),nvl($("hidItemNo").value,0)) : 
									 getItemMortgagees(nvl($("hidItemPolicyId").value,0),nvl($("hidItemNo").value,0));
		});
		
		$("btnVehiclePicOrVid").observe("click", function(){
			showAttachmentList();
		});
		
	}catch(e){
		showErrorMessage("Motor Item Info", e);
	}
</script>
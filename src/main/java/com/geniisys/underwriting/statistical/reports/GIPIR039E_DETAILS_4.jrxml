	<queryString>
		<![CDATA[SELECT   fi_item_grp, SUM (policy_no) policy_no, SUM (sum_tsi) sum_tsi,
         SUM (sum_prem_amt) sum_prem_amt, zone_type, zone_grp1,
         trty_type_cd, item_grp_name
    FROM (SELECT fi_item_grp, policy_no, sum_tsi, sum_prem_amt,
                 zone_type, zone_grp1, trty_type_cd, item_grp_name
            FROM TABLE (gipir039e_pkg.get_gipir039e_v2 ($P{P_ZONE_TYPE},
                                                        $P{P_TRTY_TYPE_CD},
                                                        $P{P_AS_OF_SW},
                                                        $P{P_USER_ID},
                                                        $P{P_FROM_DATE},
                                                        $P{P_TO_DATE},
                                                        $P{P_AS_OF_DATE}
                                                       )
                       )
           WHERE zone_grp1 = $P{P_ZONE_GRP}
          UNION
          SELECT DISTINCT fi_item_grp, '0' policy_no, 0 sum_tsi,
                          0 sum_prem_amt,
                          TO_NUMBER ($P{P_ZONE_TYPE}) zone_type,
                          TO_CHAR($P{P_ZONE_GRP}) zone_grp1, TO_CHAR($P{P_TRTY_TYPE_CD}) trty_type_cd,
                          item_grp_name
                     FROM TABLE
                              (gipir039e_pkg.get_gipir039e_v2
                                                             ($P{P_ZONE_TYPE},
                                                              $P{P_TRTY_TYPE_CD},
                                                              $P{P_AS_OF_SW},
                                                              $P{P_USER_ID},
                                                              $P{P_FROM_DATE},
                                                              $P{P_TO_DATE},
                                                              $P{P_AS_OF_DATE}
                                                             )
                              )
          UNION
          SELECT 'T' fi_item_grp, TO_CHAR(SUM(policy_no)) policy_no, SUM(sum_tsi) sum_tsi,
                          SUM(sum_prem_amt) sum_prem_amt,
                          TO_NUMBER ($P{P_ZONE_TYPE}) zone_type,
                          TO_CHAR($P{P_ZONE_GRP}) zone_grp1, TO_CHAR($P{P_TRTY_TYPE_CD}) trty_type_cd,
                          'Total' item_grp_name
                     FROM TABLE
                              (gipir039e_pkg.get_gipir039e_v2
                                                             ($P{P_ZONE_TYPE},
                                                              $P{P_TRTY_TYPE_CD},
                                                              $P{P_AS_OF_SW},
                                                              $P{P_USER_ID},
                                                              $P{P_FROM_DATE},
                                                              $P{P_TO_DATE},
                                                              $P{P_AS_OF_DATE}
                                                             )
                              )
           WHERE zone_grp1 = $P{P_ZONE_GRP}
                              )
GROUP BY fi_item_grp,
         zone_type,
         zone_grp1,
         trty_type_cd,
         item_grp_name
ORDER BY zone_grp1, fi_item_grp]]>

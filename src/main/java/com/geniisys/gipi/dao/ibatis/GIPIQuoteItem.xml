  	<procedure id="setGIPIQuoteItem"	parameterClass="com.geniisys.gipi.entity.GIPIQuoteItem">
  		DECLARE
  		BEGIN
	  		Gipi_Quote_Item_Pkg.set_gipi_quote_item(
	  		#quoteId#,    #itemNo#,         #itemTitle#,  #itemDesc#,   #currencyCd#,  #currencyRate#,          
	        #packLineCd#, #packSublineCd#,  #tsiAmt#,     #premAmt#,    #cpiRecNo#,    #cpiBranchCd#,          
	        #coverageCd#, #mcMotorNo#,      #mcPlateNo#,  #mcSerialNo#, #dateFrom#,    #dateTo#,                
	        #annPremAmt#, #annTsiAmt#,      #changedTag#, #compSw#,     #discountSw#,  #groupCd#,               
	        #itemDesc2#,  #itemGrp#,        #otherInfo#,  #packBenCd#,  #prorateFlag#, #recFlag#,              
	        #regionCd#,   #shortRtPercent#, #surchargeSw#
	  		);
  		END;
 	</procedure>
  
 	<procedure id="saveGIPIQuoteItem"	parameterClass="com.geniisys.gipi.entity.GIPIQuoteItem">
  		DECLARE
  			quoteItem gipi_quote_item%ROWTYPE;
  		BEGIN
  			quoteItem.quote_id			:= #quoteId#;
  			quoteItem.item_no			:= #itemNo#;
  			quoteItem.item_title		:= #itemTitle#;
  			quoteItem.item_desc			:= #itemDesc#;
  			quoteItem.currency_cd		:= #currencyCd#;
  			quoteItem.currency_rate		:= #currencyRate#;
	        quoteItem.pack_line_cd		:= #packLineCd#;
	        quoteItem.pack_subline_cd	:= #packSublineCd#;  
	        quoteItem.tsi_amt 			:= #tsiAmt#;     
	        quoteItem.prem_amt			:= #premAmt#;    
	        quoteItem.cpi_rec_no		:= #cpiRecNo#;    
	        quoteItem.cpi_branch_cd		:= #cpiBranchCd#;          
	        quoteItem.coverage_cd		:= #coverageCd#; 
	        quoteItem.mc_motor_no		:= #mcMotorNo#;      
	        quoteItem.mc_plate_no		:= #mcPlateNo#;  
	        quoteItem.mc_serial_no		:= #mcSerialNo#; 
	        quoteItem.date_from			:= #dateFrom#;    
	        quoteItem.date_to			:= #dateTo#;                
	        quoteItem.ann_prem_amt		:= #annPremAmt#; 
	        quoteItem.ann_tsi_amt		:= #annTsiAmt#;      
	        quoteItem.changed_tag		:= #changedTag#; 
	        quoteItem.comp_sw			:= #compSw#;     
	        quoteItem.discount_sw		:= #discountSw#;  
	        quoteItem.group_cd			:= #groupCd#;               
	        quoteItem.item_desc2		:= #itemDesc2#;  
	        quoteItem.item_grp			:= #itemGrp#;        
	        quoteItem.other_info		:= #otherInfo#;  
	        quoteItem.pack_ben_cd		:= #packBenCd#;  
	        quoteItem.prorate_flag		:= #prorateFlag#; 
	        quoteItem.rec_flag			:= #recFlag#;              
	        quoteItem.region_cd			:= #regionCd#;   
	        quoteItem.short_rt_percent	:= #shortRtPercent#; 
	        quoteItem.surcharge_sw		:= #surchargeSw#;
	        GIPI_QUOTE_ITEM_PKG.set_gipi_quote_item(quoteItem);

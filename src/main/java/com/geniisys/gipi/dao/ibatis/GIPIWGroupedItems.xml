	</select>
	
	<select id="getGIPIWGroupedItemsTableGrid" parameterClass="java.util.HashMap" resultMap="GroupedItemsByItemTableGridResultMap">
		SELECT *
		  FROM (SELECT rownum rownum_,
					   b.count_,
					   a.*
				  FROM (SELECT *
						  FROM TABLE(gipi_wgrouped_items_pkg.get_gipi_wgrouped_items_tg(#parId#, #itemNo#, #groupedItemTitle#, #remarks#))
						  <isNotNull property="notIn">
						  	WHERE grouped_item_no NOT IN $notIn$
						  </isNotNull>
						  <isNull property="sortColumn">
							ORDER BY grouped_item_no ASC
						  </isNull>
						  <isNotNull property="sortColumn">
							<isEqual property="sortColumn" compareValue="groupedItemNo">
								ORDER BY grouped_item_no
							</isEqual>
							<isEqual property="sortColumn" compareValue="groupedItemTitle">
								ORDER BY grouped_item_title
							</isEqual>
							<isEqual property="sortColumn" compareValue="principalCd">
								ORDER BY principal_cd
							</isEqual>
							<isEqual property="sortColumn" compareValue="packageCd">
								ORDER BY package_cd
							</isEqual>
							<isEqual property="sortColumn" compareValue="paytTermsDesc">
								ORDER BY payt_terms_desc
							</isEqual>
							<isEqual property="sortColumn" compareValue="fromDate">
								ORDER BY from_date
							</isEqual>
							<isEqual property="sortColumn" compareValue="toDate">
								ORDER BY to_date
							</isEqual>
							<isEqual property="sortColumn" compareValue="tsiAmt">
								ORDER BY tsi_amt
							</isEqual>
							<isEqual property="sortColumn" compareValue="premAmt">
								ORDER BY prem_amt
							</isEqual>
							<isEqual property="sortColumn" compareValue="amountCovered">
								ORDER BY amount_covered
							</isEqual>
							<isEqual property="sortColumn" compareValue="remarks">
								ORDER BY remarks
							</isEqual>
							<isEqual property="ascDescFlg" compareValue="ASC">
								ASC
							</isEqual>
							<isEqual property="ascDescFlg" compareValue="DESC">
								DESC
							</isEqual>
						</isNotNull>) a,
						(SELECT COUNT(*) count_
						   FROM TABLE(gipi_wgrouped_items_pkg.get_gipi_wgrouped_items_tg(#parId#, #itemNo#, #groupedItemTitle#, #remarks#))
						   	<isNotNull property="notIn">
						  		WHERE grouped_item_no NOT IN $notIn$
						  	</isNotNull>) b)
		 WHERE rownum_ BETWEEN #from# AND #to#
	</select>
	

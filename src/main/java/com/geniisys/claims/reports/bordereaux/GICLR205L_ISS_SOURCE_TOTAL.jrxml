	<queryString>
		<![CDATA[SELECT SUM ((NVL (c.loss_reserve, 0) - NVL (c.losses_paid, 0))
           ) outstanding_loss
  FROM gicl_res_brdrx_extr a, giis_intermediary b, gicl_res_brdrx_ds_extr c
 WHERE a.buss_source = b.intm_no(+)
   AND a.session_id = $P{P_SESSION_ID}
   AND a.claim_id = NVL ($P{P_CLAIM_ID}, a.claim_id)
   AND (NVL (c.loss_reserve, 0) - NVL (c.losses_paid, 0)) > 0
   AND a.iss_cd = $P{P_ISS_CD}
   AND DECODE (a.iss_cd, 'RI', 'RI', 'DI') = $P{P_BUSS_SOURCE_TYPE}
   AND a.buss_source = $P{P_BUSS_SOURCE}
   and a.session_id = c.session_id
   and a.BRDRX_RECORD_ID = c.BRDRX_RECORD_ID]]>
	</queryString>
